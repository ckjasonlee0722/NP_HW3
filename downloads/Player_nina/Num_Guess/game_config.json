{
    "meta": {
        "game_name": "Num_Guess",
        "version": "1.0.3",
        "author": "jason",
        "description": "終極密碼對戰！猜數字決勝負！",
        "min_players": 2,
        "max_players": 3
    },
    "execution": {
        "server": {
            "executable": "python",
            "script": "server.py",
            "arguments": {
                "port": "--port",
                "users": "--users"
            },
            "custom_args": {
                "host": "0.0.0.0"
            }
        },
        "client": {
            "executable": "python",
            "script": "client.py",
            "arguments": {
                "ip": "--host",
                "port": "--port",
                "user_id": "--user-id",
                "role": "--role"
            }
        }
    }
}