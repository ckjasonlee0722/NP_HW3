{
    "meta": {
        "game_name": "Click_War",
        "version": "1.0.12",
        "author": "jason",
        "description": "連點大戰！看誰手速快！(2-3人)",
        "min_players": 2,
        "max_players": 3
    },
    "execution": {
        "server": {
            "script": "server.py",
            "arguments": {
                "port": "--port",
                "users": "--users"
            },
            "custom_args": {
                "host": "0.0.0.0"
            }
        },
        "client": {
            "script": "client.py",
            "arguments": {
                "host": "--host",
                "port": "--port",
                "user_id": "--user-id"
            }
        }
    }
}